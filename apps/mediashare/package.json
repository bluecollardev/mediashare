{
  "name": "mediashare",
  "version": "0.1.6",
  "private": true,
  "scripts": {
    "app:bump-version": "npx react-native-version --never-amend",
    "app:ios:clean-pods": "cd ios && bash ./purge-pod-artifacts.sh && cd ..",
    "app:ios:install-pods": "cd ios && pod install --clean-install && cd ..",
    "app:ios:refresh-pods": "yarn app:ios:clean-pods && yarn app:ios:install-pods",
    "app:modules:rebuild": "rm -rf ./node_modules && yarn install",
    "link:react-native-config": "react-native link react-native-config",
    "run:android:local": "react-native run-android --variant=debug",
    "run:android:staging": "react-native run-android --variant=staging",
    "run:android:prod": "react-native run-android --variant=release",
    "run:android:logger": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "run:ios:local": "react-native run-ios --scheme 'Mediashare Development'",
    "run:ios:staging": "react-native run-ios --scheme 'Mediashare Pro'",
    "run:ios:prod": "react-native run-ios --scheme 'Mediashare Pro'",
    "serve:ios:local": "yarn app:ios:refresh-pods && react-native run-ios --scheme 'Mediashare Development'",
    "serve:ios:prod": "yarn app:ios:refresh-pods && react-native run-ios --scheme 'Mediashare Pro'",
    "test": "jest",
    "utils:lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "utils:sort-pkg": "npx sort-package-json",
    "utils:update": "git pull --rebase",
    "utils:adb:map": "adb reverse tcp:5000 tcp:5000"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "preset": "react-native"
  },
  "dependencies": {
    "@expo-google-fonts/poppins": "^0.2.2",
    "@expo/vector-icons": "12.0.2",
    "@react-native-async-storage/async-storage": "^1.15.6",
    "@react-native-community/netinfo": "^6.0.0",
    "@react-native-picker/picker": "^2.4.2",
    "@react-navigation/material-bottom-tabs": "^6.2.2",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.7.0",
    "@react-navigation/stack": "^6.2.2",
    "@reduxjs/toolkit": "^1.5.0",
    "amazon-cognito-identity-js": "^5.2.0",
    "aws-amplify": "^4.3.27",
    "aws-amplify-react-native": "^6.0.5",
    "aws-sdk": "^2.973.0",
    "expo-document-picker": "9.2.4",
    "expo-font": "9.2.1",
    "expo-secure-store": "10.2.0",
    "expo-video-thumbnails": "5.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-final-form": "6.5.2",
    "react-hook-form": "^7.34.0",
    "react-native": "0.63.4",
    "react-native-config": "^1.4.4",
    "react-native-fs": "^2.18.0",
    "react-native-gesture-handler": "1.9.0",
    "react-native-get-random-values": "^1.7.0",
    "react-native-image-picker": "^4.0.6",
    "react-native-loading-spinner-overlay": "^2.0.0",
    "react-native-pager-view": "^5.4.4",
    "react-native-paper": "^5.0.0-rc.3",
    "react-native-safe-area-context": "3.1.9",
    "react-native-screens": "2.16.1",
    "react-native-sectioned-multi-select": "^0.9.1",
    "react-native-switch-selector": "^2.1.4",
    "react-native-tab-view": "^3.1.1",
    "react-native-unimodules": "0.14.8",
    "react-native-url-polyfill": "^1.3.0",
    "react-native-vector-icons": "7.1.0",
    "react-native-video": "5.1.1",
    "react-native-zss-rich-text-editor": "^1.1.0",
    "react-redux": "7.2.6",
    "recompose": "^0.30.0",
    "redux": "4.1.2",
    "redux-thunk": "2.3.0",
    "remeda": "^0.0.30",
    "rxjs": "^6.6.6",
    "styled-components": "^5.3.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/cli-platform-android": "^8.0.5",
    "@react-native-community/cli-platform-ios": "^8.0.4",
    "@react-native-community/eslint-config": "1.1.0",
    "@types/jest": "25.2.3",
    "@types/react-native": "^0.65.5",
    "@types/react-native-vector-icons": "6.4.6",
    "@types/react-navigation": "latest",
    "@types/react-redux": "7.1.23",
    "@types/react-test-renderer": "16.9.2",
    "@types/redux-form": "8.3.1",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "babel-jest": "25.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "6.5.1",
    "jest": "25.1.0",
    "metro-react-native-babel-preset": "0.59.0",
    "react-native-cli": "^2.0.1",
    "react-test-renderer": "16.13.1",
    "redux-devtools-extension": "2.13.8",
    "remote-redux-devtools": "0.5.16",
    "remote-redux-devtools-on-debugger": "0.8.3",
    "typescript": "~4.3.5"
  }
}
