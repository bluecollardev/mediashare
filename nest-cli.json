{
  "projects": {
    "utility": {
      "type": "library",
      "root": "libs/utility",
      "entryFile": "index",
      "sourceRoot": "libs/utility/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utility/tsconfig.lib.json"
      }
    },
    "media-api": {
      "type": "application",
      "root": "apps/media-api",
      "entryFile": "main",
      "sourceRoot": "apps/media-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/media-api/tsconfig.app.json",
        "plugins": [
          "@nestjs/swagger"
        ]
      }
    }
  },
  "compilerOptions": {
    "webpack": true
  }
}
