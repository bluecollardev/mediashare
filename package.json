{
  "name": "@mediashare/source",
  "version": "0.2.0",
  "license": "MIT",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "install:tools:mongocli": "brew install mongocli",
    "gen:client-api": "npm run gen:models && npm run format",
    "gen:models": "openapi-generator-cli generate",
    "gen:certs": "mkdir -p ./certs && rm -rf ./certs/* && mkcert -key-file ./certs/key.pem -cert-file ./certs/cert.pem localhost 0.0.0.0 127.0.0.1 ",
    "seed:users": "ts-node --project scripts/tsconfig.json -r tsconfig-paths/register scripts/gen-users.script.ts",
    "media-svc:serve": "nx serve media-svc",
    "media-svc:serve:prod": "nx serve media-svc --env production",
    "user-svc:serve": "nx serve user-svc",
    "user-svc:serve:prod": "nx serve user-svc --env production",
    "docker": "npm run docker:base && docker compose -f ./docker/docker-compose.yml up --build",
    "docker:base": "docker build -t mediashare-base -f ./docker/dockerfiles/base.dockerfile .",
    "docker:media-svc:development": "PORT=3000 nx serve media-svc --host=0.0.0.0 --configuration=development --skip-nx-cache",
    "docker:media-svc:production": "PORT=3000 nx serve media-svc --host=0.0.0.0 --configuration=production --skip-nx-cache",
    "docker:user-svc:development": "PORT=3001 nx serve user-svc --host=0.0.0.0 --configuration=development --skip-nx-cache",
    "docker:user-svc:production": "PORT=3001 nx serve user-svc --host=0.0.0.0 --configuration=production --skip-nx-cache",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:watch": "jest --watch",
    "test": "nx test",
    "update": "nx migrate latest",
    "lint": "nx workspace-lint && nx lint",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "format": "nx format:write"
  },
  "private": true,
  "dependencies": {
    "@automapper/classes": "^8.7.7",
    "@automapper/core": "^8.7.7",
    "@automapper/nestjs": "^8.7.7",
    "@automapper/types": "^6.3.1",
    "@aws-sdk/client-cognito-identity-provider": "^3.363.0",
    "@nestjs-cognito/auth": "^1.0.4",
    "@nestjs/common": "^10.0.2",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.2",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.2",
    "@nestjs/swagger": "^7.0.12",
    "@nestjs/typeorm": "^10.0.0",
    "amazon-cognito-identity-js": "^6.3.1",
    "axios": "^1.0.0",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "connect-mongo": "^5.0.0",
    "express-session": "^1.17.3",
    "faker": "^5.5.3",
    "joi": "^17.9.2",
    "jwks-rsa": "^3.0.1",
    "mongodb": "^5.6.0",
    "nestjs-pino": "^3.3.0",
    "node-ses": "^3.0.3",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "remeda": "^1.23.0",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs-cognito/core": "^1.1.0",
    "@nestjs-cognito/testing": "^1.0.5",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/eslint-plugin": "16.4.2",
    "@nx/jest": "16.4.2",
    "@nx/js": "16.4.2",
    "@nx/linter": "16.4.2",
    "@nx/nest": "16.4.2",
    "@nx/node": "16.4.2",
    "@nx/webpack": "16.4.2",
    "@nx/workspace": "16.4.2",
    "@types/faker": "^6.6.9",
    "@types/jest": "^29.4.0",
    "@types/node": "~18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "^29.4.1",
    "jest-auto-stub": "^1.0.8",
    "jest-environment-node": "^29.4.1",
    "node-mocks-http": "^1.12.2",
    "nx": "16.4.2",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.1.3"
  }
}
